partdr3 <- mean(-((predA1 * data$A) / data$ps)) - mean(-((predA0 * (1 - data$A)) / (1 - data$ps)))
mods <- t(c(outcome_formula,exposure_formula))
output <- as.data.frame(cbind(mods, outcome$coefficients[2], example_ipw$coefficients[2], round(mean(part1), tabledigits), round(mean(part2), tabledigits), round(mean(-part3), tabledigits), round(mean(part1+part2+part3),tabledigits)))
