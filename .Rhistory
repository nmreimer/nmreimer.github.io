Z <- rnorm(n, 1, 2) # Z
W <- rbinom(n, 1, .5) # W
X <- rnorm(n, 2*W, 2) # X
M <- rnorm(n, 20, 1) # M
N <- rnorm(n, 10, 2) # N
log_odds_A <- .5 * Z + .5 * X - .1 * N
odds_A <- exp(log_odds_A)
p_A <- odds_A / (1 + odds_A)
A <- rbinom(n, 1, p_A) # A
Y <- rnorm(n, A + W + 2 * Z + 0.05 * M + rnorm(n, 2, 1), .25) # Y
data <- data.frame(A = (A), Y = Y, Z = Z, M = M, W = (W), X = X)
